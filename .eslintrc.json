{
    "plugins": [ "react" ],
    "extends": ["eslint:all", "plugin:react/all", "airbnb"],
    "rules": {
	    "indent": ["error", 4, {"SwitchCase": 1}],
	    "max-len": ["error", 120],
		"no-trailing-spaces": ["error", { "skipBlankLines": true }],
		"object-curly-spacing": ["error", "always"]
	}
}
